<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center_vertical"
    tools:context="com.dobi.live.SetupActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="请输入拉流地址"
        android:layout_marginLeft="10dp"
        android:visibility="visible"
        />

    <EditText
        android:id="@+id/et_pull0"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:hint=""
        android:textColor="#000000"
        android:text="rtmp://live.hkstv.hk.lxdns.com/live/hks"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="请输入拉流地址"
        android:layout_marginLeft="10dp"
        android:visibility="visible"
        />
    <EditText
        android:id="@+id/et_pull1"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:hint=""
        android:textColor="#000000"
        android:text="rtmp://202.69.69.180:443/webcast/bshdlive-pc"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="请输入拉流地址"
        android:layout_marginLeft="10dp"
        android:visibility="visible"
        />
    <EditText
        android:id="@+id/et_pull2"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:hint=""
        android:textColor="#000000"
        android:text="rtmp://mobliestream.c3tv.com:554/live/goodtv.sdp"
        />

    <Button
        android:id="@+id/btn_enter"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="进入"
        />

</LinearLayout>
